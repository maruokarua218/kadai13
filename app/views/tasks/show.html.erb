<p id="notice"><%= notice %></p>

<p>
  <strong>タイトル</strong>
  <%= @task.title %>
</p>

<p>
  <strong>コンテント</strong>
  <%= @task.content %>
</p>

<p>
  <strong>ステータス</strong>
  <%= @task.status %>
